{
	"name": "microsoft/microsoft-graph-beta",
	"type": "library",
	"description": "The Microsoft Graph Beta SDK for PHP",
	"homepage": "https://developer.microsoft.com/en-us/graph",
	"license": "MIT",
	"authors": [
		{
			"name": "Microsoft Graph Client Tooling",
			"email": "graphtooling@service.microsoft.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.0 || ^7.3",
		"microsoft/microsoft-graph-core": "dev-dev",
		"ext-json": "*"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0 || ^9.0",
		"phpstan/phpstan": "^0.12.90"
	},
	"autoload": {
		"psr-4": {
			"Beta\\Microsoft\\Graph\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Microsoft\\Graph\\Test\\": "tests/"
		}
	}
}
